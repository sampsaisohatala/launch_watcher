 // fetch next available launch\n   useEffect(() => {\n      fetch(allLaunchUrl)\n         .then((res) => res.json())\n         .then((json) => {\n            setAllLaunches(json);\n         });\n      fetch(allPadsUrl)\n         .then((res) => res.json())\n         .then((json) => {\n            setAllPads(json);\n         });\n   }, []);\n\n   useEffect(() => {\n      if (allLaunches) filteredLaunchHandler();\n   }, [allLaunches, filter]);\n\n   const filteredLaunchHandler = () => {\n      let launches;\n      let sortedLaunches;\n      console.log(new Date(), ' / ', allLaunches[0].launch_date_utc);\n\n      switch (filter) {\n         case 'future':\n            launches = allLaunches.filter((launch) => new Date() < new Date(launch.launch_date_utc));\n            sortedLaunches = launches.sort((a, b) => a.launch_date_unix - b.launch_date_unix);\n            setFilteredLaunches(sortedLaunches);\n            break;\n\n         case 'past':\n            launches = allLaunches.filter((launch) => new Date() > new Date(launch.launch_date_utc));\n            sortedLaunches = launches.sort((a, b) => b.launch_date_unix - a.launch_date_unix);\n            setFilteredLaunches(sortedLaunches);\n            break;\n\n         default:\n            sortedLaunches = allLaunches.sort((a, b) => a.launch_date_unix - b.launch_date_unix);\n            setFilteredLaunches(sortedLaunches);\n            break;\n      }\n   };\n\n   return (\n      <div className=\"launches\">\n         <h1 className=\"text-center\">{`${filter} launches`.toUpperCase()}</h1>\n         <Filter setFilter={setFilter} />\n         <div className=\"m-5\">\n            <Table striped bordered hover variant=\"dark\">\n               <thead>\n                  <tr>\n                     <th>Mission Name</th>\n                     <th>Launch date</th>\n                     <th>Rocket</th>\n                     <th>Launch site</th>\n                  </tr>\n               </thead>\n               <tbody>\n                  {filteredLaunches.map((launch) => (\n                     <tr key={launch.flight_number + launch.mission_name}>\n                        <td>{launch.mission_name}</td>\n                        <td>{launch.launch_date_utc.slice(0, -1).split('T')[0]}</td>\n                        <td>{launch.rocket.second_stage.block ? `${launch.rocket.rocket_name} Block ${launch.rocket.second_stage.block}` : `${launch.rocket.rocket_name}`}</td>\n                        <td>{allPads.map((pad) => (pad.site_id === launch.launch_site.site_id ? pad.location.name : ''))}</td>\n                     </tr>\n                  ))}\n               </tbody>\n            </Table>\n         </div>\n      </div>\n   );\n}\n\nexport default LaunchesView;\n","/Users/sampsa/Programming/launch_watcher/src/components/HomePage.js",["54","55","56","57"],"/Users/sampsa/Programming/launch_watcher/src/components/NotFoundPage.js",[],"/Users/sampsa/Programming/launch_watcher/src/reducers/launches.js",[],"/Users/sampsa/Programming/launch_watcher/src/actions/launches.js",[],"/Users/sampsa/Programming/launch_watcher/src/store/configureStore.js",[],{"ruleId":"58","severity":1,"message":"59","line":26,"column":7,"nodeType":"60","endLine":26,"endColumn":9,"suggestions":"61"},{"ruleId":"58","severity":1,"message":"62","line":32,"column":7,"nodeType":"60","endLine":32,"endColumn":28,"suggestions":"63"},{"ruleId":"64","severity":1,"message":"65","line":4,"column":10,"nodeType":"66","messageId":"67","endLine":4,"endColumn":19},{"ruleId":"64","severity":1,"message":"68","line":4,"column":21,"nodeType":"66","messageId":"67","endLine":4,"endColumn":29},{"ruleId":"64","severity":1,"message":"69","line":5,"column":8,"nodeType":"66","messageId":"67","endLine":5,"endColumn":19},{"ruleId":"64","severity":1,"message":"70","line":9,"column":10,"nodeType":"66","messageId":"67","endLine":9,"endColumn":20},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'calculateTimeLeft'. Either include it or