ntainer\">\n            <Link className=\"navigation__nav-link\" to=\"/\">\n               Home\n            </Link>\n            <Link className=\"navigation__nav-link\" to=\"/happenings\">\n               Happenings\n            </Link>\n            <button className=\"navigation__hamburger\" onClick={() => setShowMenu(!showMenu)}>\n               <FontAwesomeIcon className=\"navigation__icon\" icon={faBars} />\n            </button>\n\n            {maskTransitions.map(({ item, key, props }) => item && <animated.div key={key} style={props} className=\"navigation__menu_mask\" onClick={() => setShowMenu(!showMenu)} />)}\n            {menuTransitions.map(\n               ({ item, key, props }) =>\n                  item && (\n                     <animated.div key={key} style={props} className=\"navigation__menu\">\n                        <Link className=\"navigation__nav-link--menu\" to=\"/\" onClick={() => setShowMenu(!showMenu)}>\n                           Home\n                        </Link>\n                        <Link className=\"navigation__nav-link--menu\" to=\"/happenings\" onClick={() => setShowMenu(!showMenu)}>\n                           Happenings\n                        </Link>\n                     </animated.div>\n                  )\n            )}\n            {/* TODO: Add delay for the fade in/out animations */}\n\n            {/* <DelayLink className=\"navigation__nav-link\" to=\"/\" delay={510} onDelayStart={handleDelayStart} onDelayEnd={handleDelayEnd}>\n               Home\n            </DelayLink>\n\n            <DelayLink className=\"navigation__nav-link\" to=\"/launches\" delay={510} onDelayStart={handleDelayStart} onDelayEnd={handleDelayEnd}>\n               Launches\n            </DelayLink> */}\n         </div>\n      </div>\n   );\n}\n\nexport default Navigation;\n","/Users/sampsa/Programming/launch_watcher/src/components/LaunchTimer.js",["96"],"import React from 'react';\nimport { useEffect, useState } from 'react';\nimport moment from 'moment';\n\nfunction NextLaunchTimer(props) {\n   const [timeLeft, setTimeLeft] = useState('');\n   const launchTime = moment(props.launchTime).valueOf();\n\n   const calculateTimeLeft = () => {\n      const difference = Math.abs(launchTime - moment().valueOf());\n      return {\n         days: Math.floor(difference / (1000 * 60 * 60 * 24)),\n         hours: ('0' + Math.floor((difference / (1000 * 60 * 60)) % 24)).slice(-2),\n         minutes: ('0' + Math.floor((difference / 1000 / 60) % 60)).slice(-2),\n         seconds: ('0' + Math.floor((difference / 1000) % 60)).slice(-2),\n      };\n   };\n\n   // call to avoid waiting before updateLoop\n   useEffect(() => {\n      setTimeLeft(calculateTimeLeft());\n   }, []);\n\n   // updateLoop\n   useEffect(() => {\n      let timer = setTimeout(() => {\n         setTimeLeft(calculateTimeLeft());\n      }, 1000);\n      return () => {\n         clearTimeout(timer);\n      };\n   });\n\n   // Create \"T- / T+\"\n   const timeFormat = () => {\n      const format = launchTime - moment().valueOf() > 0 ? '-' : '+';\n      return (\n         <div className={!props.individual ? 'timer__semicolon' : 'timer__semicolon--alt'} key=\"timeformat\">\n            T{format}{' '}\n         </div>\n      );\n   };\n\n   const timerComponents = [timeFormat()];\n\n   // Create timer components\n   Object.keys(timeLeft).forEach((interval) => {\n      if (!timeLeft[interval]) {\n         return;\n      }\n      timerComponents.push(\n         // Add timer unit with number and text\n         <div key={interval} className=\"timer__unit\">\n            <span className={!props.individual ? 'timer__unit_number' : 'timer__unit_number--alt'}>{timeLeft[interval]}</span>\n            <span className={!props.individual ? 'timer__unit_text' : 'timer__unit_text--alt'}>{interval}</span>\n         </div>\n      );\n\n      if (interval !== 'seconds') {\n         timerComponents.push(\n            // Add semicolon\n            <div key={interval + ':'} className={!props.individual ? 'timer__semicolon' : 'timer__semicolon--alt'}>\n               {' : '}\n            </div>\n         );\n      }\n   });\n\n   return <div className={!props.individual ? 'timer' : 'timer--alt'}>{timerComponents.length ? timerComponents : <span className=\"timer__not-available\">No new launch data available...</span>}</div>;\n}\n\nexport default NextLaunchTimer;\n",["97","98"],"/Users/sampsa/Programming/launch_watcher/src/components/HomePage.js",[],"/Users/sampsa/Programming/launch_watcher/src/components/NotFoundPage.js",[],"/Users/sampsa/Programming/launch_watcher/src/store/configureStore.js",[],"/Users/sampsa/Programming/launch_watcher/src/routers/AppRouter.js",[],["99","100"],"/Users/sampsa/Programming/launch_watcher/src/reducers/filters.js",[],"/Users/sampsa/Programming/launch_watcher/src/components/Loadingscreen.js",[],"/Users/sampsa/Programming/launch_watcher/src/components/HappeningsPage.js",[],["101","102"],"/Users/sampsa/Programming/launch_watcher/src/components/HappeningCard.js",[],"/Users/sampsa/Programming/launch_watcher/src/reducers/happenings.js",[],"/Users/sampsa/Programming/launch_watcher/src/selectors/happenings.js",["103"],"export const getNextHappening = (happenings) => {\n   for (let index = 0; index < happenings.length; index++) {\n      const difference = new Date(happenings[index].net).getTime() - Date.now();\n      if (difference > 0) return happenings[index];\n   }\n};\n\nexport const getVisibleHappenings = (happenings, { category }) => {\n   return happenings.filter((happening) => {\n      if (category === 'all') return happening;\n      else if (category === 'launches') return happening.launch;\n      else if (category === 'events') return !happening.launch;\n   });\n};\n",["104","105"],"/Users/sampsa/Programming/launch_watcher/src/actions/happenings.js",[],"/Users/sampsa/Programming/launch_watcher/src/actions/filters.js",[],"/Users/sampsa/Programming/launch_watcher/src/hooks/useImagePlaceholder.js",[],"/Users/